[input_devices]

buffering/agile_event_flushing=true
pointing/emulate_touch_from_mouse=true

[physics]

common/physics_fps=70
common/enable_pause_aware_picking=true
3d/physics_engine="GodotPhysics"
3d/default_gravity=32.035
3d/default_linear_damp=0.0
3d/default_angular_damp=0.0

[rendering]

threads/thread_model=2
threads/thread_safe_bvh=true
vram_compression/import_etc=true
quality/shadow_atlas/quadrant_3_subdiv=6
quality/shadows/filter_mode=2
quality/reflections/irradiance_max_size=64
quality/shading/force_vertex_shading.mobile=false
quality/shading/force_lambert_over_burley.mobile=false
quality/shading/force_blinn_over_ggx.mobile=false
quality/filters/anisotropic_filter_level=16
gles2/compatibility/disable_half_float.iOS=false
quality/subsurface_scattering/quality=2
quality/subsurface_scattering/scale=8
quality/subsurface_scattering/follow_surface=true
quality/voxel_cone_tracing/high_quality=true
quality/reflections/atlas_size=260
quality/reflections/atlas_subdiv=32
quality/filters/msaa=4
environment/default_environment="res://default_env.tres"
quality/dynamic_fonts/use_oversampling=false
