[display]

window/size/width=2560
window/size/height=1400
window/size/borderless=true
window/size/fullscreen=true

[editor_plugins]

enabled=PoolStringArray( "res://addons/vitavehicle_ui/plugin.cfg", "res://addons/zylann.hterrain/plugin.cfg" )

[global]

anti=true
taa=true

